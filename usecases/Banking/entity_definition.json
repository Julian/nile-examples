{
  "name": "Banking",
  "schema": {
    "type": "object",
    "properties": {
      "accountType": {
        "type": "string",
        "enum": ["checking", "savings", "money management"]
      },
      "accountID": {
        "type": "string"
      },
      "firstName": {
        "type": "string"
      },
      "lastName": {
        "type": "string"
      },
      "status": {
        "type": "string",
        "enum": ["Submitted", "Provisioning", "Up", "Deleted"]
      }
    },
    "required": [
      "accountType"
    ]
  }
}
